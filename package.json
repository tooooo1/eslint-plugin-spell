{
  "name": "eslint-plugin-spell",
  "version": "0.0.0",
  "description": "ESLint plugin to detect and report misspellings against",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "keywords": [
    "eslint",
    "plugin",
    "spell",
    "guard"
  ],
  "files": [
    "dist",
    "lib"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch"
  },
  "author": {
    "name": "Chung-il Jung",
    "email": "jungchungil7@gmail.com"
  },
  "devDependencies": {
    "@types/eslint": "^8.44.7",
    "eslint": "^8.53.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "eslint": ">=0.8.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
